{"ast":null,"code":"//  Компонент с фото и текстом о студенте  //\nimport'./AboutMeContent.css';import aboutmeimage from'../../images/aboutme.jpg';import{jsx as _jsx}from\"react/jsx-runtime\";import{jsxs as _jsxs}from\"react/jsx-runtime\";var AboutMeContent=function AboutMeContent(){return/*#__PURE__*/_jsxs(\"div\",{className:\"aboutme\",children:[/*#__PURE__*/_jsxs(\"div\",{className:\"aboutme__description\",children:[/*#__PURE__*/_jsx(\"h3\",{className:\"aboutme__name\",children:\"\\u041A\\u0438\\u0440\\u0438\\u043B\\u043B\"}),/*#__PURE__*/_jsx(\"p\",{className:\"aboutme__status\",children:\"\\u0421\\u0442\\u0443\\u0434\\u0435\\u043D\\u0442 \\u042F\\u043D\\u0434\\u0435\\u043A\\u0441.\\u041F\\u0440\\u0430\\u043A\\u0442\\u0438\\u043A\\u0443\\u043C, \\u043F\\u0440\\u043E\\u0434\\u0430\\u043A\\u0442, \\u043C\\u0430\\u0440\\u043A\\u0435\\u0442\\u043E\\u043B\\u043E\\u0433, 48 \\u043B\\u0435\\u0442\"}),/*#__PURE__*/_jsxs(\"p\",{className:\"aboutme__text\",children:[\"\\u0416\\u0438\\u0432\\u0443 \\u0438 \\u0440\\u0430\\u0431\\u043E\\u0442\\u0430\\u044E \\u0432 \\u041C\\u043E\\u0441\\u043A\\u0432\\u0435. \\u041E\\u0431\\u0440\\u0430\\u0437\\u043E\\u0432\\u0430\\u043D\\u0438\\u0435 \\u0432\\u044B\\u0441\\u0448\\u0435\\u0435 \\u0442\\u0435\\u0445\\u043D\\u0438\\u0447\\u0435\\u0441\\u043A\\u043E\\u0435 (\\u041C\\u0413\\u0410\\u0425\\u041C). \\u041B\\u044E\\u0431\\u043B\\u044E \\u043F\\u0443\\u0442\\u0435\\u0448\\u0435\\u0441\\u0442\\u0432\\u043E\\u0432\\u0430\\u0442\\u044C, \\u0431\\u0435\\u0433\\u0430\\u0442\\u044C, \\u043F\\u043B\\u0430\\u0432\\u0430\\u0442\\u044C, \\u043F\\u0440\\u0438\\u043E\\u0431\\u0440\\u0435\\u0442\\u0430\\u0442\\u044C \\u0446\\u0435\\u043D\\u043D\\u044B\\u0435 \\u043D\\u0430\\u0432\\u044B\\u043A\\u0438. \\u0412\\u0435\\u0440\\u0441\\u0442\\u043A\\u043E\\u0439 \\u0438 \\u0440\\u0430\\u0437\\u0440\\u0430\\u0431\\u043E\\u0442\\u043A\\u043E\\u0439 \\\"\\u043D\\u0430 \\u043A\\u043E\\u043B\\u0435\\u043D\\u043A\\u0435\\\" \\u0437\\u0430\\u043D\\u0438\\u043C\\u0430\\u044E\\u0441\\u044C \\u0434\\u0430\\u0432\\u043D\\u043E, \\u0432\\u043F\\u0435\\u0440\\u0432\\u044B\\u0435 \\u0443\\u0447\\u0443\\u0441\\u044C \\u0441\\u0438\\u0441\\u0442\\u0435\\u043C\\u043D\\u043E. \\u041F\\u043E\\u0447\\u0442\\u0438 30-\\u043B\\u0435\\u0442\\u043D\\u0438\\u0439 \\u043E\\u043F\\u044B\\u0442 \\u0440\\u0430\\u0431\\u043E\\u0442\\u044B \\u0432 \\u0418\\u0422 \\u0432 \\u0440\\u043E\\u0441\\u0441\\u0438\\u0439\\u0441\\u043A\\u0438\\u0445 \\u0438 \\u0437\\u0430\\u0440\\u0443\\u0431\\u0435\\u0436\\u043D\\u044B\\u0445 \\u043A\\u043E\\u043C\\u043F\\u0430\\u043D\\u0438\\u044F\\u0445. \\u0423\\u0432\\u043B\\u0435\\u043A\\u0430\\u044E\\u0441\\u044C \\u0442\\u0435\\u043C\\u043E\\u0439 \\u0441\\u0442\\u0430\\u0440\\u0442\\u0430\\u043F\\u043E\\u0432 \\u0432 \\u0440\\u043E\\u043B\\u0438 \\u043F\\u0440\\u043E\\u0434\\u0430\\u043A\\u0442\\u0430 \\u0438 \\u043C\\u0430\\u0440\\u043A\\u0435\\u0442\\u043E\\u043B\\u043E\\u0433\\u0430, \\u0440\\u0435\\u0436\\u0435 - \\u0438\\u043D\\u0432\\u0435\\u0441\\u0442\\u043E\\u0440\\u0430 \\u0438\\u043B\\u0438 \\u0442\\u0440\\u0435\\u043A\\u0435\\u0440\\u0430.\",/*#__PURE__*/_jsx(\"strike\",{children:\"\\u041C\\u0435\\u0447\\u0442\\u0430\\u044E\"}),\" \\u041F\\u043B\\u0430\\u043D\\u0438\\u0440\\u0443\\u044E \\u043D\\u0430\\u0439\\u0442\\u0438 \\u043A\\u043E\\u043C\\u0430\\u043D\\u0434\\u0443 \\u0435\\u0434\\u0438\\u043D\\u043E\\u043C\\u044B\\u0448\\u043B\\u0435\\u043D\\u043D\\u0438\\u043A\\u043E\\u0432 \\u0438 \\u0432\\u043C\\u0435\\u0441\\u0442\\u0435 \\u043F\\u043E\\u0441\\u0442\\u0440\\u043E\\u0438\\u0442\\u044C \\\"\\u0435\\u0434\\u0438\\u043D\\u043E\\u0440\\u043E\\u0433\\u0430\\\".\"]}),/*#__PURE__*/_jsx(\"a\",{className:\"link aboutme__link\",href:\"https://github.com/kirillchistov/\",target:\"_blank\",rel:\"noreferrer\",children:\"Github\"}),/*#__PURE__*/_jsx(\"a\",{className:\"link aboutme__link\",href:\"https://fb.me/kirchistov\",target:\"_blank\",rel:\"noreferrer\",children:\"Facebook\"})]}),/*#__PURE__*/_jsx(\"img\",{className:\"aboutme__image\",src:aboutmeimage,alt:\"\\u0424\\u043E\\u0442\\u043E\"})]});};export default AboutMeContent;","map":{"version":3,"names":["aboutmeimage","AboutMeContent"],"sources":["/Users/kirillchistov/Projects/movies-explorer-frontend/src/components/AboutMeContent/AboutMeContent.jsx"],"sourcesContent":["//  Компонент с фото и текстом о студенте  //\nimport './AboutMeContent.css';\nimport aboutmeimage from '../../images/aboutme.jpg';\n\nconst AboutMeContent = () => {\n  return (\n    <div className='aboutme'>\n      <div className='aboutme__description'>\n        <h3 className='aboutme__name'>Кирилл</h3>\n        <p className='aboutme__status'>\n          Студент Яндекс.Практикум, продакт, маркетолог, 48 лет\n        </p>\n        <p className='aboutme__text'>\n          Живу и работаю в Москве. Образование высшее техническое (МГАХМ).\n          Люблю путешествовать, бегать, плавать, приобретать ценные навыки.\n          Версткой и разработкой \"на коленке\" занимаюсь давно, впервые учусь системно. \n          Почти 30-летний опыт работы в ИТ в российских и зарубежных компаниях.\n          Увлекаюсь темой стартапов в роли продакта и маркетолога, реже - инвестора или трекера.\n          <strike>Мечтаю</strike> Планирую найти команду единомышленников и вместе построить \"единорога\".\n        </p>\n        <a\n          className='link aboutme__link'\n          href='https://github.com/kirillchistov/'\n          target='_blank'\n          rel='noreferrer'\n        >\n          Github\n        </a>\n        <a\n          className='link aboutme__link'\n          href='https://fb.me/kirchistov'\n          target='_blank'\n          rel='noreferrer'\n        >\n          Facebook\n        </a>\n      </div>\n      <img className='aboutme__image' src={aboutmeimage} alt='Фото' />\n    </div>\n  );\n}\n\nexport default AboutMeContent;\n"],"mappings":"AAAA;AACA,MAAO,sBAAsB,CAC7B,MAAOA,aAAY,KAAM,0BAA0B,CAAC,wFAEpD,GAAMC,eAAc,CAAG,QAAjBA,eAAc,EAAS,CAC3B,